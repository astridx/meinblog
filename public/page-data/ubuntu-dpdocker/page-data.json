…","fields":{"slug":"/ubuntu-dpdocker/"},"frontmatter":{"title":"DPDocker einrichten","date":"2022-04-11","tags":["Linux","Ubuntu","DPDocker","Joomla"],"description":"desc","thumbnail":{"childImageSharp":{"fixed":{"base64":"data:image/png;base64,","width":150,"height":150,"src":"/static/85b1f306e08dd767f16b4e40ab643cdb/92ab1/dp_logo.png","srcSet":"/static/85b1f306e08dd767f16b4e40ab643cdb/92ab1/dp_logo.png 1x"}}}}}},"pageContext":{"slug":"/ubuntu-dpdocker/","previous":{"id":"862a0675-8ba6-58ea-8cb7-4341e6ffb391","frontmatter":{"title":"Docker Compose unter Ubuntu 20.04 einrichten","tags":["Linux","Ubuntu","Docker","Docker Compose"],"categories":["Betriebssystem"],"template":"post"},"fields":{"slug":"/ubuntu-docker-compose-einrichten/"}},"next":{"id":"adee6d5b-4cc2-5723-9b65-ee3f78b9af82","frontmatter":{"title":"Eine eigene Joomla-Erweiterung mit DPDocker","tags":["Linux","Ubuntu","DPDocker","Joomla"],"categories":["Betriebssystem"],"template":"post"},"fields":{"slug":"/joomla-erweiterung-mit-dpdocker/"}}}},
