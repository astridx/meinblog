atter":{"title":"Kontrollierte Komponenten in React","date":"2021-08-03","tags":["JavaScript","React"],"syndication":null,"description":"desc","thumbnail":null}}},"pageContext":{"slug":"/react-lernen-react-modern-kontrollierte-komponenten/","previous":{"id":"834edd71-6a55-59a1-8df4-e4c42327751d","frontmatter":{"title":"Datenabruf mit React","tags":["JavaScript","React"],"categories":["React"],"template":"post"},"fields":{"slug":"/react-lernen-react-modern-datenabruf/"}},"next":{"id":"79cf153b-309f-57ea-be0d-5dfe1f30192e","frontmatter":{"title":"Custom-Hooks (fortgeschrittene Anleitung)","tags":["JavaScript","React"],"categories":["React"],"template":"post"},"fields":{"slug":"/react-lernen-react-modern-custom-hooks/"}}}},
